{
  "name": "@evryg/integresql",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "description": "An effect-ts based client of the integresql tool used for creating isolated potgresql databases used in integration tests",
  "homepage": "https://www.evryg.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/evryg-org/effect-contrib.git",
    "directory": "packages/integresql"
  },
  "bugs": {
    "url": "https://github.com/evryg-org/effect-contrib/issues"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "provenance": true
  },
  "scripts": {
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "fast-glob": "^3.3.2"
  },
  "peerDependencies": {
    "@effect/schema": "^0.75.4",
    "effect": "^3.9.2",
    "vitest": "^2.0.5",
    "@effect/platform-node": "^0.63.6"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.13.2",
    "testcontainers": "^10.13.2",
    "@effect/platform": "^0.68.6",
    "@effect/platform-node": "^0.63.6",
    "@effect/vitest":  "^0.12.1",
    "@effect/sql-pg": "^0.16.6",
    "@effect/schema": "^0.75.4",
    "effect": "^3.9.2",
    "@types/pg": "^8.11.10",
    "vitest": "^2.0.5",
    "@types/node": "^22.7.5",
    "knex": "^3.1.0",
    "pg": "^8.13.0",
    "typescript": "^5.6.3"
  }
}
